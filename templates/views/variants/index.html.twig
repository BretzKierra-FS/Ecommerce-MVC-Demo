{% extends "layouts/base.html.twig" %} 


{% block main_content %}
    <h2>All Variants</h2>
    <ul>
        {% for variant in variants %}
            <li>
                {{variant.title}} - 
                <a href="/variants/{{ variant.id }}"> Edit</a>
              
            </li>
            
        {% endfor %}
    </ul>
    <a href="/variants/new">Create New Variant</a>
{% endblock %}
